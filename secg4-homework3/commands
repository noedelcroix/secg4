  490  openssl genrsa -out rsa.private 1024
  491  openssl rsa -in rsa.private -out rsa.public -pubout -outform PEM
  492  rm *
  493  openssl genrsa -out private.key 1024
  494  openssl req -new -x509 -key private.key -out publickey.cer -days 365
  495  openssl pkcs12 -export -out public_privatekey.pfx -inkey private.key -in publickey.cer
  496  openssl x509 -pubkey -noout -in be_ea_cert.pem  > be_ea_key.pem
  497  ls
